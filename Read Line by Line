import java.io.*;
class Lineby
{
 public static void main(String args[])throws IOException
{
	FileReader isr =new FileReader("lineby.txt");
 	BufferedReader br= new BufferedReader(isr);
 	String s;
	 while((s=br.readLine())!=null)
 	{
  	  System.out.println(s);
 	}
	br.close();
 	isr.close();      
}
 
}
